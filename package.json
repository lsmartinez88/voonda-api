{
  "name": "voonda-api",
  "version": "1.0.0",
  "description": "API completa para gestión de vehículos con autenticación JWT - Prisma ORM",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "prisma generate",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "prisma:db:push": "prisma db push",
    "prisma:db:pull": "prisma db pull",
    "prisma:seed": "node prisma/seed.js",
    "db:reset": "prisma db push --force-reset && npm run prisma:seed",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@supabase/supabase-js": "^2.38.5",
    "axios": "^1.13.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "prisma": "^6.18.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "api",
    "nodejs",
    "express",
    "prisma",
    "postgresql",
    "jwt",
    "vehiculos",
    "rest-api",
    "orm"
  ],
  "author": "Voonda Team",
  "license": "MIT"
}
